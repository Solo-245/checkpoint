package main

import "fmt"

func main() {
	for i := '0'; i <= '7'; i++ {
		for j := i + 1; j <= '8'; j++ {
			for k := j + 1; k <= '9'; k++ {

				// Print the combination as characters
				fmt.Printf("%c%c%c", i, j, k)

				// Print comma and space unless it's the last combination
				if !(i == '7' && j == '8' && k == '9') {
					fmt.Printf(", ")
				}
			}
		}
	}
}
